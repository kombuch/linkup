{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": "off",
    "no-use-before-define": "off",
    "react/jsx-props-no-spreading": "off",
    "object-curly-newline": "off",
    "semi": "off",
    "implicit-arrow-linebreak": "off"
  }
}
